@article{bandeiramorais2021economiccomplexityinequality,
  title = {Economic {{Complexity}} and {{Inequality}}: {{Does Regional Productive Structure Affect Income Inequality}} in {{Brazilian States}}?},
  shorttitle = {Economic {{Complexity}} and {{Inequality}}},
  author = {Bandeira Morais, Margarida and Swart, Julia and Jordaan, Jacob Arie},
  year = {2021},
